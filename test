? text : `(${(['</span></font></font>一<font face="Liberation Serif, serif"><font size="3" style="font-size: 12pt"><span lang="en-US">','</span></font></font>二<font face="Liberation Serif, serif"><font size="3" style="font-size: 12pt"><span lang="en-US">','</span></font></font>三<font face="Liberation Serif, serif"><font size="3" style="font-size: 12pt"><span lang="en-US">','</span></font></font>四<font face="Liberation Serif, serif"><font size="3" style="font-size: 12pt"><span lang="en-US">','</span></font></font>五<font face="Liberation Serif, serif"><font size="3" style="font-size: 12pt"><span lang="en-US">','</span></font></font>六<font face="Liberation Serif, serif"><font size="3" style="font-size: 12pt"><span lang="en-US">'])[idx]})
